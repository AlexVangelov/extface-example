<div class="page-header nav">
  <%= link_to 'Sign up', new_shop_registration_path, class: 'btn btn-default navbar-btn pull-right' %>
  <h3 class='navbar-text'>
    Shop &raquo; Sign in
  </h3>
</div>

<%= form_for resource, as: resource_name, url: session_path(resource_name), html: { class: 'form-horizontal' } do |f| %>
  <%= form_group f, :name, :text_field, autofocus: true, value: :example  %>

  <%= form_group f, :password, :password_field, autocomplete: "off", value: '1' %>

  <% if devise_mapping.rememberable? -%>
    <div class='form-group'>
      <div class='col-sm-offset-2 col-sm-10 col-md-8'>
        <%= f.label :remmember_me, class: 'checkbox' do %>
          <%= f.check_box :remember_me %> Remember me
        <% end %>
      </div>
    </div>
  <% end %>
  
  <div class='form-group'>
    <div class='col-sm-offset-2 col-sm-10 col-md-8'>
      <%= f.submit "Sign in", class: 'btn btn-default' %>
    </div>
  </div>
<% end %>
